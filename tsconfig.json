{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2021",
    "types": ["node", "@types/gtag.js"]
  },
  "exclude": ["dist", "node_modules"]
}
